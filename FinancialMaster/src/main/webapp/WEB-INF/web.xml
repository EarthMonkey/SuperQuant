    <servlet-name>HomePageServlet</servlet-name>
    <servlet-class>servlet.main.HomePageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomePageServlet</servlet-name>
    <url-pattern>/ToHomePageServlet</url-pattern>
  </servlet-mapping>
 
